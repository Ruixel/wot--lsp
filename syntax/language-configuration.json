{
	"comments": {
		"lineComment": "#",
		"blockComment": ["#[", "]"]
	},
	// symbols used as brackets
	"brackets": [
		["{", "}"],
		["(", ")"]
	],
	// symbols that are auto closed when typing
	"autoClosingPairs": [
		["{", "}"], // Block statement
		["[", "]"], // For multi-line expressions
		["(", ")"], // Function params
		["'", "'"],
		["\"", "\""]
	],
	// symbols that can be used to surround a selection
	"surroundingPairs": [
		["{", "}"],
		["(", ")"],
		["'", "'"],
		["\"", "\""]
	]
}
