{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Wot++",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.wotpp",
					"match": "\\b(let)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.wotpp",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.wotpp",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.wpp"
}
